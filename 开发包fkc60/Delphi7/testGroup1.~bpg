#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = test.exe test.CHT
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

test.exe: test.dpr
  $(DCC)

test.CHT: CHT\test.dpr
  $(DCC)


